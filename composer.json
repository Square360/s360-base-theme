{
    "name": "square360/s360_base_theme",
    "description": "Square360 Base Theme",
    "type": "drupal-theme",
    "homepage": "https://bitbucket.org/sq360_sysadmin/s360-base-theme",
    "keywords": [
        "Drupal",
        "Theme"
    ],
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "scripts": {
        "code-sniff": [
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,info --ignore=node_modules,bower_components,vendor,ui .",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info --ignore=node_modules,bower_components,vendor,ui ."
        ]
    },
    "require-dev": {
        "drupal/core": "^11",
        "drupal/coder": "^8.3",
        "squizlabs/php_codesniffer": "3.*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "drupal/core-composer-scaffold": false
        }
    }
}
