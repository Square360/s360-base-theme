{
  "name": "s360-base-theme",
  "client": "S360 Base Theme",
  "version": "1.0.0",
  "description": "Storybook development for Drupal themes.",
  "keywords": [
    "component library",
    "design system",
    "drupal",
    "pattern library",
    "storybook",
    "styleguide"
